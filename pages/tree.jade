extends _layout
mixin list(folder)
  ul
    for item, key in folder
      if typeof item === "object" && key !== "ajax"
        li.folder
          button
            = key
          mixin list(item)
      else if key !== treeFile && key !== "ajax"
        li.document
          a(href=item)
            = key

block content

  style.
    @font-face{font-family:'tree';src:url(data:application/x-font-woff;charset=utf-8;base64,) format('woff'),url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');font-weight:normal;font-style:normal;}
    #tree ul{padding:0;list-style:none}
    #tree li{padding-left:1em;}
    #tree button{background:none;border:0 none;padding:0;}
    #tree a{display:inline-block;}
    .folder{margin-bottom:10px;}
    .document:before,.folder:before{font-family:'tree';speak:none;font-style:normal;font-weight:normal;font-variant:normal;text-transform:none;line-height:1;-webkit-font-smoothing:antialiased;}
    .document:before{content:"\64";font-size:0.875em;line-height:1;padding-left:4px;padding-right:6px;}
    .folder:before{content:"\66";font-size:1.25em;line-height:1;vertical-align:middle;}
    .header ul{text-align:left}

  div#tree.sct-listing
    div.header
      h1.ttl
        = "arborescence du site "+ title
      +list(tree)

block footer

block scripts
